<Window x:Class="WpfApp1.Libary"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Libary" Height="450" Width="800">
	<Grid>
		<Label Content="The Libary" HorizontalAlignment="Center" Height="48" Margin="0,26,0,0" VerticalAlignment="Top" Width="140" FontSize="22" FontWeight="Bold"/>
		<ListView x:Name="listBox" HorizontalAlignment="Center" Height="256" Margin="0,79,0,0" VerticalAlignment="Top" Width="504" d:ItemsSource="{d:SampleData ItemCount=5}"/>
		<Button Content="Back" HorizontalAlignment="Left" Height="41" Margin="551,351,0,0" VerticalAlignment="Top" Width="101" Click="BackButton_Click"/>
	</Grid>
</Window>